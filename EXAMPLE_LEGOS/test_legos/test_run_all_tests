#---------------------------------------------------------------------------------------------------------------
header "test_Ar_multiple_models"              \
       "   |"                                 \
       "   |-- model_ArNe_P_MLJ_NEIGH_RVEC_F" \
       "   |-- model_Ar_P_MLJ_C"              \
       "   |-- model_Ar_P_MLJ_F90"            \
       "   |-- model_Ar_P_MLJ_NEIGH_RVEC_F"   \
       "   |-- model_Ar_P_MMorse"             \
       "   |"
echo ""
echo "NOTE: Only two examples are executed here because all other permutations produce identical results"
echo ""
