       "   |-- model_Ar_P_MMorse"           \
       "   |"

divider "test_Ar_FCCcohesive_MI_OPBC --- model_Ar_P_MLJ_C"
echo model_Ar_P_MLJ_C           | ./test_Ar_FCCcohesive_MI_OPBC/test_Ar_FCCcohesive_MI_OPBC

divider "test_Ar_FCCcohesive_MI_OPBC --- model_Ar_P_MLJ_F90"
echo model_Ar_P_MLJ_F90         | ./test_Ar_FCCcohesive_MI_OPBC/test_Ar_FCCcohesive_MI_OPBC

divider "test_Ar_FCCcohesive_MI_OPBC --- model_Ar_P_MLJ_MI_OPBC_H_F"
echo model_Ar_P_MLJ_MI_OPBC_H_F | ./test_Ar_FCCcohesive_MI_OPBC/test_Ar_FCCcohesive_MI_OPBC

divider "test_Ar_FCCcohesive_MI_OPBC --- model_Ar_P_MMorse"
echo model_Ar_P_MMorse          | ./test_Ar_FCCcohesive_MI_OPBC/test_Ar_FCCcohesive_MI_OPBC
footer

#---------------------------------------------------------------------------------------------------------------
